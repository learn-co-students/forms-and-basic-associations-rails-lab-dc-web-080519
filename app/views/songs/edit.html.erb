<h1>Editing <%= @song.title %></h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br><br>

  <%= f.label :artist_name %><br>
  <%= f.text_field :artist_name %><br><br>

  <%= f.label :genre %><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br><br>

  <%= f.submit %>
<% end %>
